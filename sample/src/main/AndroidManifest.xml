<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="testlibuiza">

    <application
        android:name="testlibuiza.app.LSApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".sample.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".sample.uizavideo.deprecatedfrm.TestUizaVideoIMActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenSize|screenLayout|smallestScreenSize|uiMode"
            android:screenOrientation="portrait" />
        <activity
            android:name=".sample.uizavideo.rl.TestUizaVideoIMActivityRl"
            android:configChanges="keyboard|keyboardHidden|orientation|screenSize|screenLayout|smallestScreenSize|uiMode"
            android:screenOrientation="portrait" />
        <activity
            android:name=".sample.api.TestAPIActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".sample.uizavideo.slide.TestUizaVideoIMActivityRlSlide"
            android:configChanges="keyboard|keyboardHidden|orientation|screenSize|screenLayout|smallestScreenSize|uiMode"
            android:screenOrientation="portrait" />

        <receiver
            android:name=".sample.uizavideo.FloatClickFullScreenReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="FLOAT_CLICKED_FULLSCREEN" />
            </intent-filter>
        </receiver>
    </application>

</manifest>